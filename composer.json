{
  "name": "joskolenberg/laravel-jory",
  "description": "Build queries based on json",
  "license": "MIT",
  "keywords": ["query", "laravel"],
  "authors": [
    {
      "name": "Jos Kolenberg",
      "email": "joskolenberg@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "laravel/framework": "^5.5",
    "joskolenberg/jory": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "JosKolenberg\\LaravelJory\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JosKolenberg\\LaravelJory\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^3.5"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/joskolenberg/jory",
      "no-api": true
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "JosKolenberg\\LaravelJory\\JoryServiceProvider"
      ]
    }
  }
}
