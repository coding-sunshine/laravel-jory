{
  "name": "joskolenberg/laravel-jory",
  "description": "Laravel package for creating Jory endpoints.",
  "license": "MIT",
  "keywords": ["query", "laravel", "jory", "json"],
  "authors": [
    {
      "name": "Jos Kolenberg",
      "email": "joskolenberg@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "laravel/framework": "^5.5",
    "joskolenberg/jory": "^0.7",
    "joskolenberg/eloquent-reflector": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "JosKolenberg\\LaravelJory\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JosKolenberg\\LaravelJory\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^3.5"
  },
  "extra": {
    "laravel": {
      "providers": [
        "JosKolenberg\\LaravelJory\\JoryServiceProvider"
      ]
    }
  }
}
